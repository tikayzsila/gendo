version: '3.7'
services:

  {{project_name}}:
    image: gitlab.space-tech.online:5050/webdev/{{project_name}}:latest
    build:
      context: ./

    restart: always
    ports:
      - "8888:3000"

    networks:
      - {{project_name}}

networks:
  {{project_name}}:
    name: {{project_name}}
    driver: bridge